############################################
# modifiy these variables to your settings:
PYTHON_DIR=C:\Programme\Python23
PYTHON_LIB=C:\Programme\Python23\libs\python23.lib
SIP = D:\sip-4.1.1\sipgen\sip.exe
SIPINC = D:\sip-4.1.1\siplib
QT_LIB = D:\qt-3.3.4\lib\qt-mt334.lib
############################################


CC = cl -c -nologo -MD -Zm1200 -TP -EHsc -GR -Zi /D "WIN32" -Od -RTC1 -GS /D "DEBUG"
LD = cl -nologo -MD
LDOPTS = /link /OPT:NOREF /DEBUG /ASSEMBLYDEBUG /MAP /MAPINFO:EXPORTS /MAPINFO:LINES /EXPORT:initBALL /DLL
BALL_LIB = ..\..\..\lib\libBall.lib ..\..\..\lib\libVIEW.lib

XLIB = kernel32.lib user32.lib gdi32.lib winspool.lib comdlg32.lib advapi32.lib shell32.lib ole32.lib oleaut32.lib uuid.lib odbc32.lib odbccp32.lib wsock32.lib ..\..\..\Windows\Contrib\ONC_RPC\librpc\oncrpc.lib $(PYTHON_LIB) $(QT_LIB)

INCL = -I ..\..\..\Windows\include -I ..\..\..\include -I ..\..\..\Windows\Contrib\regex-0.12 -I ..\..\..\Windows\Contrib\ONC_RPC -I $(SIPINC) -I $(PYTHON_DIR)\include -I $(QTDIR)\include -I ..\ -I SIP

SIP_OPTS=-c SIP -e -s ".C" -b BALL.sbf

o = .obj

.c.obj:
	$(CC) $(CFLAGS) $(DFLAGS) $(IFLAGS) $(INCL) $(*).C

