BALL_INLINE
ConnectionObject *
ConnectionObject::getParent() const
	throw()
{
	return parent_;
}

BALL_INLINE
void 
ConnectionObject::setParent_(ConnectionObject &object)
	throw()
{
	parent_ = &object;
}

BALL_INLINE
void 
ConnectionObject::clearParent_()
	throw()
{
	parent_ = 0;
}
